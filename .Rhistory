setwd("C:/Users/NeuralNet/Downloads")
library(rpart)
library(dplyr)
library(modelr)
titanic_data <- read.csv("train.csv")
target <- "Survived"
features <- c("Age", "Sex")
features <- paste(features, collapse="+")
formula <- as.formula(paste(target,"~",features,sep = ""))
splitData <- resample_partition(titanic_data, c(test = 0.3, train = 0.7))
fit2 <- rpart(formula, data = splitData$train)
mae(model = fit2, data = splitData$test)
plot(fit2, uniform=TRUE)
text(fit2, cex=.6)
